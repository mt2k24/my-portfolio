<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Projects - <%= name %></title>
  <link rel="stylesheet" href="/styles.css">
</head>
<body>
  <%- include('partials/header') %>
  <section>
    <h2>Projects</h2>
    <% projects.forEach(project => { %>
      <div class="project">
        <h3><%= project.title %></h3>
        <p><%= project.description %></p>
      </div>
    <% }) %>
  </section>
  <%- include('partials/footer') %>
</body>
</html>
